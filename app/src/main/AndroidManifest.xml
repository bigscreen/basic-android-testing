        </activity>
        <activity
            android:name=".lessons.one.LessonOneActivity"
            android:label="@string/lesson_one" />
        <activity
            android:name=".lessons.two.LessonTwoActivity"
            android:label="@string/lesson_two" />
    </application>

</manifest>